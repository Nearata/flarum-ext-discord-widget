{
    "name": "nearata/flarum-ext-discord-widget",
    "description": "Adds a floating Discord widget.",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "Unlicense",
    "require": {
        "flarum/core": "^1.7"
    },
    "authors": [
        {
            "name": "Nearata",
            "email": "nearata@protonmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nearata\\DiscordWidget\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Discord Widget",
            "category": "feature",
            "icon": {
                "name": "fab fa-discord",
                "color": "#fff",
                "backgroundColor": "#7289da"
            }
        }
    }
}
